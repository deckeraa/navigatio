        <PAGEOBJECT XPOS="118.0008" YPOS="75770.9934369427" OwnPage="181" ItemID="1414317182" PTYPE="4" WIDTH="540" HEIGHT="263.235668789806" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="0" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="1" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L540 0 L540 263.236 L0 263.236 L0 0 Z" copath="M0 0 L540 0 L540 263.236 L0 263.236 L0 0 Z" gXpos="118.0008" gYpos="75770.9934369427" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="-1" BACKITEM="-1">
            <StoryText>
                <DefaultStyle/>
                <para/>
                <ITEXT CH="[36]     Porrō"/>
                <ITEXT FEATURES="inherit superscript" CH="1"/>
                <ITEXT CH=" praedictus prōcūrātor, fīnītā refectiōne, dīxit: “Deō adiuvante, revertar ad vōs, in diē adventūs Sānctī Spīritūs super apostolōs"/>
                <ITEXT FEATURES="inherit superscript" CH="2"/>
                <ITEXT CH=", cum dispendiīs vestrīs.” [37] Acceptā benedictiōne sānctī patris et omnium quī cum eō erant, reversus est"/>
                <ITEXT FEATURES="inherit superscript" CH="3"/>
                <ITEXT CH=" in locum suum. Porrō venerābilis pater mānsit ibīdem praedictōs diēs. [38] Cōnsummātīs itaque diēbus fēstīs, sānctus Brendānus frātribus suīs praecēpit preparāre nāvigium et implēre vāscula ex fonte. [39] Ducta iam nāvis ad mare, ecce praedictus socius"/>
                <ITEXT FEATURES="inherit superscript" CH="4"/>
                <ITEXT CH=" cum nāvī onerātā ēscīs ad frātrēs venit. [40] Cumque haec omnia posuisset in nāviculā sānctī virī, ōsculātīs omnibus, reversus est unde vēnerat."/>
                <para PARENT="Latin"/>
            </StoryText>
        </PAGEOBJECT>
        <PAGEOBJECT XPOS="118.0008" YPOS="72923.28" OwnPage="173" ItemID="1417423662" PTYPE="4" WIDTH="540" HEIGHT="138.815541401287" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="0" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="1" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L540 0 L540 138.816 L0 138.816 L0 0 Z" copath="M0 0 L540 0 L540 138.816 L0 138.816 L0 0 Z" gXpos="118.0008" gYpos="72923.28" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="-1" BACKITEM="-1">
            <StoryText>
                <DefaultStyle/>
                <ITEXT FEATURES="inherit superscript" CH="1"/>
                <ITEXT FONT="DejaVu Serif Italic" CH="This verse now having been finished. "/>
                <ITEXT CH="This is an ablative absolute."/>
                <para PARENT="footnotes"/>
                <ITEXT FEATURES="inherit superscript" CH="2"/>
                <ITEXT FONT="DejaVu Serif Italic" CH="He did service to them for three days"/>
                <ITEXT CH=". A literal translation of obsequium from the dictionary is difficult here, so I recommend translating more idiomatically given the context."/>
                <para PARENT="footnotes"/>
                <ITEXT FEATURES="inherit superscript" CH="3"/>
                <ITEXT FONT="DejaVu Serif Italic" CH="The order of the Sabbath day being finishing, and the spiritual victims have been immolated to God, and the dinner being consumed, the same procurator said to Saint Brendan."/>
                <ITEXT CH=" This is an ablative absolute."/>
                <para PARENT="footnotes"/>
                <ITEXT FEATURES="inherit superscript" CH="4"/>
                <ITEXT FONT="DejaVu Serif Italic" CH="The benediction having been given"/>
                <ITEXT CH=". This is an ablative absolute."/>
                <trail PARENT="footnotes"/>
            </StoryText>
        </PAGEOBJECT>
