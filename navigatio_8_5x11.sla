        <PAGEOBJECT XPOS="118.0008" YPOS="102870.000295541" OwnPage="245" ItemID="1879056974" PTYPE="4" WIDTH="540" HEIGHT="182.828025477706" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="0" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="1" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L540 0 L540 182.828 L0 182.828 L0 0 Z" copath="M0 0 L540 0 L540 182.828 L0 182.828 L0 0 Z" gXpos="118.0008" gYpos="102870.000295541" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="-1" BACKITEM="-1">
            <StoryText>
                <DefaultStyle/>
                <ITEXT FEATURES="inherit superscript" CH="1"/>
                <ITEXT FONT="DejaVu Serif Italic" CH="On a certain day, the dean having designated a place of burial for a certain dead (person), that I might bury (him)"/>
                <ITEXT CH=". dēfūnctō here is in the dative."/>
                <para PARENT="footnotes"/>
                <ITEXT FEATURES="inherit superscript" CH="2"/>
                <ITEXT FONT="DejaVu Serif Italic" CH="For yesterday I migrated from this world, and this here is the place of my burial."/>
                <ITEXT CH=" St. Patrick died the day before and is buried in the spot where Paul was told to dig."/>
                <para PARENT="footnotes"/>
                <ITEXT FEATURES="inherit superscript" CH="3"/>
                <ITEXT FONT="DejaVu Serif Italic" CH="I I found it just as he had foretold to me."/>
                <para PARENT="footnotes"/>
                <ITEXT FEATURES="inherit superscript" CH="4"/>
                <ITEXT FONT="DejaVu Serif Italic" CH="Immediatly I saw it (the boat) furrowing a swift course through the waves of the sea, that it might return to it's fatherland."/>
                <trail PARENT="footnotes"/>
            </StoryText>
        </PAGEOBJECT>
        <PAGEOBJECT XPOS="151.0008" YPOS="103546.983113371" OwnPage="247" ItemID="1879611550" PTYPE="5" WIDTH="450" HEIGHT="1" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PCOLOR2="Black" PLINEART="1" path="" gXpos="151.0008" gYpos="99386.9831133734" gWidth="0" gHeight="0" LAYER="0"/>
        <PAGEOBJECT XPOS="118.0008" YPOS="103557.901584713" OwnPage="247" ItemID="1879616622" PTYPE="4" WIDTH="540" HEIGHT="295.337579617844" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="0" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="1" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L540 0 L540 295.338 L0 295.338 L0 0 Z" copath="M0 0 L540 0 L540 295.338 L0 295.338 L0 0 Z" gXpos="118.0008" gYpos="103557.901584713" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="-1" BACKITEM="-1">
            <StoryText>
                <DefaultStyle/>
                <ITEXT FEATURES="inherit superscript" CH="1"/>
                <ITEXT FONT="DejaVu Serif Italic" CH="And a bundle of tinder/kindling for making a fire"/>
                <ITEXT CH=". faciendum is a gerundive."/>
                <para PARENT="footnotes"/>
                <ITEXT FEATURES="inherit superscript" CH="2"/>
                <ITEXT FONT="DejaVu Serif Italic" CH="And striking a flint, I made fire with the tinder/kindling/grasses"/>
                <ITEXT CH=". Perhaps this detail is meant to evoke the imagery of Moses striking the rock in the wilderness in Numbers 20."/>
                <para PARENT="footnotes"/>
                <ITEXT FEATURES="inherit superscript" CH="3"/>
                <ITEXT FONT="DejaVu Serif Italic" CH="Where I could take drink and fill my vessel for the work of my hands."/>
                <ITEXT CH=" The Dickinson commentary suggests glossing this as "/>
                <ITEXT FONT="DejaVu Serif Italic" CH="for washing my hands"/>
                <ITEXT CH="."/>
                <trail PARENT="footnotes"/>
            </StoryText>
        </PAGEOBJECT>
