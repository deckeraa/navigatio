        <PAGEOBJECT XPOS="118" YPOS="4579.56687898089" OwnPage="9" ItemID="1334343947" PTYPE="4" WIDTH="540" HEIGHT="355.872611464968" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="0" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="1" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L540 0 L540 355.873 L0 355.873 L0 0 Z" copath="M0 0 L540 0 L540 355.873 L0 355.873 L0 0 Z" gXpos="118" gYpos="4579.56687898089" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="-1" BACKITEM="-1">
            <StoryText>
                <DefaultStyle/>
                <ITEXT FEATURES="inherit superscript" CH="1"/>
                <ITEXT FONT="DejaVu Serif Italic" CH="Because today our souls have been satisfied by the divine refreshment"/>
                <ITEXT CH=". satiātae sunt is a construction of a past passive participle + sunt, which means &quot;has been satisifed&quot;."/>
                <para PARENT="footnotes"/>
                <ITEXT FEATURES="inherit superscript" CH="2"/>
                <ITEXT FONT="DejaVu Serif Italic" CH="Now having finished dinner, they began to carry out the work of God"/>
                <ITEXT CH=". Fīnītā iam cēnā is an ablative absolute."/>
                <para PARENT="footnotes"/>
                <ITEXT FEATURES="inherit superscript" CH="3"/>
                <ITEXT FONT="DejaVu Serif Italic" CH="Having finished everything, the man of God and those who were with him gave their bodies to rest until the third watch of the night"/>
                <ITEXT CH=". Hīs omnibus fīnīttīs is an ablative absolute."/>
                <para PARENT="footnotes"/>
                <ITEXT FEATURES="inherit superscript" CH="4"/>
                <ITEXT CH="Psalm 50:17"/>
                <para PARENT="footnotes"/>
                <ITEXT FEATURES="inherit superscript" CH="5"/>
                <ITEXT CH="Psalm 148:10. In this context, virtūtēs appears to refer to a particular class of angelic beings."/>
                <para PARENT="footnotes"/>
                <ITEXT FEATURES="inherit superscript" CH="6"/>
                <ITEXT CH="Psalm 89:17"/>
                <trail PARENT="footnotes"/>
            </StoryText>
        </PAGEOBJECT>
