        <PAGEOBJECT XPOS="118.0008" YPOS="76045.2354751592" OwnPage="181" ItemID="1418221470" PTYPE="4" WIDTH="540" HEIGHT="144" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="0" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="1" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L540 0 L540 144 L0 144 L0 0 Z" copath="M0 0 L540 0 L540 144 L0 144 L0 0 Z" gXpos="118.0008" gYpos="76045.2354751592" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="-1" BACKITEM="-1">
            <StoryText>
                <DefaultStyle/>
                <ITEXT FEATURES="inherit superscript" CH="1"/>
                <ITEXT FONT="DejaVu Serif Italic" CH="Also the aforesaid procurator, having finished dining, said"/>
                <ITEXT CH="."/>
                <para PARENT="footnotes"/>
                <ITEXT FEATURES="inherit superscript" CH="2"/>
                <ITEXT CH="The day of the coming of the Holy Spirit on the Apostles is Pentecost. See Acts 2:1-31."/>
                <para PARENT="footnotes"/>
                <ITEXT FEATURES="inherit superscript" CH="3"/>
                <ITEXT FONT="DejaVu Serif Italic" CH="He returned to his place"/>
