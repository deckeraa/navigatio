        <PAGEOBJECT XPOS="118.0008" YPOS="25015.2313579618" OwnPage="59" ItemID="1850833406" PTYPE="4" WIDTH="540" HEIGHT="264.152866242039" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="0" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="1" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L540 0 L540 264.153 L0 264.153 L0 0 Z" copath="M0 0 L540 0 L540 264.153 L0 264.153 L0 0 Z" gXpos="118.0008" gYpos="25015.2313579618" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="-1" BACKITEM="-1">
            <StoryText>
                <DefaultStyle/>
                <para/>
                <tab/>
                <ITEXT CH="Sed praedictus barbarus occurrit ad lītus cum aliīs. Portāvit forcipem in manibus cum massā igneā dē scōriā, immēnsae magnitūdinis atque fervōris. Barbarus iactāvit praedictam massam super famulōs Chrīstī, sed massam nōn pulsāvit eōs. Massam cecidit in mare et coepit fervere, et fūmus ascendēbat dē mare."/>
                <para PARENT="Latin"/>
                <tab/>
                <ITEXT CH="Tunc aliī barbarī occurrērunt ad lītus et iactābant massās in mare post famulōs Chrīstī. Mare aestuābat sī cācabus plēnus carnibus."/>
                <para PARENT="Latin"/>
                <tab/>
                <ITEXT CH="Frātrēs audiēbant ululātum magnam per tōtum diem ab illā īnsulā. Sānctus Pater cōnfortābat suōs monachōs et dīxit: “Ō mīlitēs Chrīstī, sumus ante portam īnfernōrum.”"/>
                <para PARENT="Latin"/>
            </StoryText>
        </PAGEOBJECT>
        <PAGEOBJECT XPOS="766.0008" YPOS="24166.0944" OwnPage="58" ItemID="1851064062" PTYPE="4" WIDTH="540" HEIGHT="736.318471337581" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="2" COLGAP="0" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="1" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L540 0 L540 736.318 L0 736.318 L0 0 Z" copath="M0 0 L540 0 L540 736.318 L0 736.318 L0 0 Z" gXpos="766.0008" gYpos="24166.0944" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="-1" BACKITEM="-1">
            <StoryText>
                <DefaultStyle/>
                <ITEXT CH="ad: to, towards, at, according to"/>
                <para PARENT="Vocab"/>
                <ITEXT CH="arbōrēs: trees; plural feminine nominative from arbor, arboris"/>
                <para PARENT="Vocab"/>
                <ITEXT CH="audīvērunt: they heard; 3rd person plural perfect active from audio, audire, audivi, auditus"/>
                <para PARENT="Vocab"/>
                <ITEXT CH="Brendānus: Brendan of Clonfert (c. AD 484 – c. 577), an Irish abbot who sailed the Atlantic"/>
                <para PARENT="Vocab"/>
                <ITEXT CH="chrīste: annointed one, Christ; singular masculine vocative from Chrīstus, Chrīstī"/>
                <para PARENT="Vocab"/>
                <ITEXT CH="chrīstī: annointed one, Christ; singular masculine genitive from Chrīstus, Chrīstī"/>
                <para PARENT="Vocab"/>
                <ITEXT CH="collīsiōnēs: collision; plural feminine nominative from collīsiō, collīsiōnis"/>
                <para PARENT="Vocab"/>
                <ITEXT CH="contrā: against, away from, facing"/>
                <para PARENT="Vocab"/>
                <ITEXT CH="cūdēs: you will pound; 2nd person singular future active from cūdō, cūdere, cūdī, cūsus"/>
                <para PARENT="Vocab"/>
                <ITEXT CH="de: of, from, away from, down from"/>
                <para PARENT="Vocab"/>
                <ITEXT CH="diēs: day; singular common nominative from diēs, diēī"/>
                <para PARENT="Vocab"/>
                <ITEXT CH="dīxit: he said; 3rd person singular perfect active from dīcō, dīcere, dīxī, dictus"/>
                <para PARENT="Vocab"/>
                <ITEXT CH="domine: lord, master; singular masculine vocative from dominus, dominī"/>
                <para PARENT="Vocab"/>
                <ITEXT CH="erat: he was; 3rd person singular imperfect active from sum, esse, fuī, futūrus"/>
                <para PARENT="Vocab"/>
                <ITEXT CH="et: and"/>
                <para PARENT="Vocab"/>
                <ITEXT CH="ex: out of, from"/>
                <para PARENT="Vocab"/>
                <ITEXT CH="fabrōrum: of smiths; plural masculine genitive from faber, fabrī"/>
                <para PARENT="Vocab"/>
                <ITEXT CH="famulōs: servants, companions; plural masculine accusative from famulus, famulī"/>
                <para PARENT="Vocab"/>
                <ITEXT CH="ferrum: iron; singular neuter nominative from ferrum, ferrī"/>
                <para PARENT="Vocab"/>
                <ITEXT CH="fīliī: sons; singular masculine genitive from fīlius, fīlī"/>
                <para PARENT="Vocab"/>
                <ITEXT CH="follium: of bellows; plural masculine genitive from follis, follis"/>
                <para PARENT="Vocab"/>
                <ITEXT CH="forās: outside (adverb)"/>
                <para PARENT="Vocab"/>
                <ITEXT CH="fugite: flee;     from fug.ite"/>
                <para PARENT="Vocab"/>
                <ITEXT CH="habēbat: he had; 3rd person singular imperfect active from habeō, habēre, habuī, habitus"/>
                <para PARENT="Vocab"/>
