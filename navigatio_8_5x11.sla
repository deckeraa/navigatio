        <PAGEOBJECT XPOS="118" YPOS="10411.1082802548" OwnPage="23" ItemID="1434798843" PTYPE="4" WIDTH="540" HEIGHT="346.700636942676" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="0" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="1" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L540 0 L540 346.701 L0 346.701 L0 0 Z" copath="M0 0 L540 0 L540 346.701 L0 346.701 L0 0 Z" gXpos="118" gYpos="10411.1082802548" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="-1" BACKITEM="-1">
            <StoryText>
                <DefaultStyle/>
                <ITEXT FEATURES="inherit superscript" CH="1"/>
                <ITEXT FONT="DejaVu Serif Italic" CH="The abbot also was exhorting the brothers with great joviality, saying: &quot;Out of this fountain which today you wanted to stealthily drink, in the same way make hospitality with pleasantness and the fear of God.&quot;"/>
                <ITEXT CH=" Hortābātur has an active meaning because it comes from a deponent verb."/>
                <para PARENT="footnotes"/>
                <ITEXT FEATURES="inherit superscript" CH="2"/>
                <ITEXT FONT="DejaVu Serif Italic" CH="Out of the other turbid fountain, which you saw, the feet of the brothers are washed every day, because it is warm all the time."/>
                <para PARENT="footnotes"/>
                <ITEXT FEATURES="inherit superscript" CH="3"/>
                <ITEXT FONT="DejaVu Serif Italic" CH="The bread which you saw, it is unknown to us where they are prepared or who carries them to our cellar."/>
                <ITEXT CH=" ignōtum + est is a past passive particle construction."/>
                <para PARENT="footnotes"/>
                <ITEXT FEATURES="inherit superscript" CH="4"/>
                <ITEXT FONT="DejaVu Serif Italic" CH="However it is known to us that, out of the great mercy of God, they (meaning the loaves) are ministered to his servants through some subject creature."/>
                <trail PARENT="footnotes"/>
            </StoryText>
        </PAGEOBJECT>
