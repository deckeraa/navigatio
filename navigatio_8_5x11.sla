        <PAGEOBJECT XPOS="118" YPOS="37497.7834394904" OwnPage="89" ItemID="346973183" PTYPE="4" WIDTH="540" HEIGHT="309.554140127439" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="0" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="1" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L540 0 L540 309.554 L0 309.554 L0 0 Z" copath="M0 0 L540 0 L540 309.554 L0 309.554 L0 0 Z" gXpos="118" gYpos="37497.7834394904" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="-1" BACKITEM="-1">
            <StoryText>
                <DefaultStyle/>
                <para/>
                <para/>
                <ITEXT CH="Caput XXIII Īnsula Fabrōrum"/>
                <para PARENT="Chapter Heading"/>
                <para/>
                <ITEXT CH="[1]     Trānsāctīs autem diēbus octo"/>
                <ITEXT FEATURES="inherit superscript" CH="1"/>
                <ITEXT CH=", vīdērunt īnsulam nōn longē, valdē rūsticam, saxōsam atque scoriōsam, sine arboribus et herbā, plēnam officīnīs fabrōrum. [2] Venerābilis pater ait frātribus suīs: “Vērē, frātrēs, angustia est mihi dē hāc īnsulā, quia nōlō in illam īre aut etiam sibi appropinquāre"/>
                <ITEXT FEATURES="inherit superscript" CH="2"/>
                <ITEXT CH="; sed ventus illūc subtrahit nōs rēctō cursū.”"/>
                <para PARENT="Latin"/>
                <ITEXT CH="[3]     Circumdabātur"/>
                <ITEXT FEATURES="inherit superscript" CH="3"/>
                <ITEXT CH=" illa līmite. Ergō, illīs praetereuntibus parumper—quasi iactus lapidis"/>
                <ITEXT FEATURES="inherit superscript" CH="4"/>
                <ITEXT CH="—audiērunt sonitūs follium sufflantium quasi tonitruum, atque malleōrum collīsiōnēs contrā ferrum et cūdēs. [4] Hīs audītis, venerābilis pater armāvit sē Dominicō trophaeō"/>
                <ITEXT FEATURES="inherit superscript" CH="5"/>
                <ITEXT CH=" in quattuor partēs, dīcēns: “Domine Iēsu Chrīste, līberā nōs dē hāc īnsulā!”"/>
                <para PARENT="Latin"/>
            </StoryText>
        </PAGEOBJECT>
        <PAGEOBJECT XPOS="118" YPOS="38310" OwnPage="91" ItemID="355705039" PTYPE="4" WIDTH="540" HEIGHT="18" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="0" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="1" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L540 0 L540 18 L0 18 L0 0 Z" copath="M0 0 L540 0 L540 18 L0 18 L0 0 Z" gXpos="118" gYpos="33318.0000000023" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="-1" BACKITEM="-1">
