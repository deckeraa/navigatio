        <PAGEOBJECT XPOS="118.0008" YPOS="75357.7354598726" OwnPage="179" ItemID="1418518078" PTYPE="4" WIDTH="540" HEIGHT="101.417406369423" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="0" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="1" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L540 0 L540 101.417 L0 101.417 L0 0 Z" copath="M0 0 L540 0 L540 101.417 L0 101.417 L0 0 Z" gXpos="118.0008" gYpos="75357.7354598726" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="-1" BACKITEM="-1">
            <StoryText>
                <DefaultStyle/>
                <ITEXT FEATURES="inherit superscript" CH="1"/>
                <ITEXT FONT="DejaVu Serif Italic" CH="Having had therefore re-seated themselves at the table, behold the aforesaid bird reseated himself on the prow of the ship."/>
                <ITEXT CH=" The subjunctive is used because this is a cum circumstantial clause."/>
                <para PARENT="footnotes"/>
                <ITEXT CH="2"/>
                <ITEXT FONT="DejaVu Serif Italic" CH="The venerable old man (i.e. Saint Brendan) having had finished these things."/>
                <ITEXT CH=" Here haec is the neuter plural accusative."/>
                <trail PARENT="footnotes"/>
            </StoryText>
        </PAGEOBJECT>
        <PAGEOBJECT XPOS="151.0008" YPOS="75347.0743133771" OwnPage="179" ItemID="1419030478" PTYPE="5" WIDTH="450" HEIGHT="1" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PCOLOR2="Black" PLINEART="1" path="" gXpos="151.0008" gYpos="71187.0743133768" gWidth="0" gHeight="0" LAYER="0"/>
