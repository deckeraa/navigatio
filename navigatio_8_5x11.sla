        <PAGEOBJECT XPOS="118.0008" YPOS="90746.1338394905" OwnPage="217" ItemID="1517242830" PTYPE="4" WIDTH="540" HEIGHT="372.847052229205" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="0" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="1" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L540 0 L540 372.847 L0 372.847 L0 0 Z" copath="M0 0 L540 0 L540 372.847 L0 372.847 L0 0 Z" gXpos="118.0008" gYpos="90746.1338394905" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="-1" BACKITEM="-1">
            <StoryText>
                <DefaultStyle/>
                <para/>
                <para/>
                <ITEXT CH="Caput XXIII "/>
                <para PARENT="Chapter Heading"/>
                <para PARENT="Chapter Heading"/>
                <ITEXT CH="Īnsula Fabrōrum"/>
                <para PARENT="Chapter Heading"/>
                <para/>
                <ITEXT CH="[1]     Trānsāctīs autem diēbus octo"/>
                <ITEXT FEATURES="inherit superscript" CH="1"/>
                <ITEXT CH=", vīdērunt īnsulam nōn longē, valdē rūsticam, saxōsam atque scoriōsam, sine arboribus et herbā, plēnam officīnīs fabrōrum. [2] Venerābilis pater ait frātribus suīs: “Vērē, frātrēs, angustia est mihi dē hāc īnsulā, quia nōlō in illam īre aut etiam sibi appropinquāre"/>
                <ITEXT FEATURES="inherit superscript" CH="2"/>
                <ITEXT CH="; sed ventus illūc subtrahit nōs rēctō cursū.”"/>
                <para PARENT="Latin"/>
                <ITEXT CH="[3]     Circumdabātur"/>
                <ITEXT FEATURES="inherit superscript" CH="3"/>
                <ITEXT CH=" illa līmite. Ergō, illīs praetereuntibus parumper—quasi iactus lapidis"/>
                <ITEXT FEATURES="inherit superscript" CH="4"/>
                <ITEXT CH="—audiērunt sonitūs follium sufflantium quasi tonitruum, atque malleōrum collīsiōnēs contrā ferrum et cūdēs. [4] Hīs audītis, venerābilis pater armāvit sē Dominicō trophaeō"/>
                <ITEXT FEATURES="inherit superscript" CH="5"/>
                <ITEXT CH=" in quattuor partēs, dīcēns: “Domine Iēsu Chrīste, līberā nōs dē hāc īnsulā!”"/>
                <para PARENT="Latin"/>
            </StoryText>
            <PageItemAttributes>
                <ItemAttribute Name="TOC" Type="none" Value="Caput XXIII Īnsula Fabrōrum" Parameter="" Relationship="none" RelationshipTo="" AutoAddTo="none"/>
            </PageItemAttributes>
        </PAGEOBJECT>
        <PAGEOBJECT XPOS="118.0008" YPOS="91558.3536000001" OwnPage="219" ItemID="1522111374" PTYPE="4" WIDTH="540" HEIGHT="18" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="0" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="1" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L540 0 L540 18 L0 18 L0 0 Z" copath="M0 0 L540 0 L540 18 L0 18 L0 0 Z" gXpos="118.0008" gYpos="86566.3536000024" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="-1" BACKITEM="-1">
