        <PAGEOBJECT XPOS="118.0008" YPOS="58663.3593579618" OwnPage="139" ItemID="1302925406" PTYPE="4" WIDTH="540" HEIGHT="350.369426751592" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="0" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="1" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L540 0 L540 350.369 L0 350.369 L0 0 Z" copath="M0 0 L540 0 L540 350.369 L0 350.369 L0 0 Z" gXpos="118.0008" gYpos="58663.3593579618" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="-1" BACKITEM="-1">
            <StoryText>
                <DefaultStyle/>
                <ITEXT FEATURES="inherit superscript" CH="1"/>
                <ITEXT FONT="DejaVu Serif Italic" CH="Sing to our God, sing, sing to our king, sing wisely"/>
                <ITEXT CH=".  Psalm 46:7 in the Vulgate."/>
                <para PARENT="footnotes"/>
                <ITEXT FEATURES="inherit superscript" CH="2"/>
                <ITEXT FONT="DejaVu Serif Italic" CH="Shine, Lord, your face upon us, and have mercy on us."/>
                <ITEXT CH="  Psalm 66:2 in the Vulgate."/>
                <para PARENT="footnotes"/>
                <ITEXT FEATURES="inherit superscript" CH="3"/>
                <ITEXT FONT="DejaVu Serif Italic" CH="Behold how good and how pleasant it is for brothers to dwell in unity."/>
                <ITEXT CH=" Psalm 133:1 in the Vulgate."/>
                <para PARENT="footnotes"/>
                <ITEXT FEATURES="inherit superscript" CH="4"/>
                <ITEXT FONT="DejaVu Serif Italic" CH="Therefore Saint Brendan was refreshing his brothers by means of the Paschal festivities until the eighth day."/>
                <para PARENT="footnotes"/>
                <ITEXT FEATURES="inherit superscript" CH="5"/>
                <ITEXT FONT="DejaVu Serif Italic" CH="Having spent therefore the festal days"/>
                <ITEXT CH="."/>
                <para PARENT="footnotes"/>
                <ITEXT FEATURES="inherit superscript" CH="6"/>
                <ITEXT FONT="DejaVu Serif Italic" CH="Let us take a stipend from this fountain, because until now it has not been of use to us, except to wash hands and feet"/>
                <ITEXT CH=". Accipiāmus is the hortatory subjunctive (&quot;let us take&quot;). We have to treat nōn fuit nōbīs opus as an idiom and pick something in English that makes sense."/>
                <para PARENT="footnotes"/>
                <ITEXT FEATURES="inherit superscript" CH="7"/>
                <ITEXT FONT="DejaVu Serif Italic" CH="And having loaded everything into the boat before the holy father, the same man spoke to them saying"/>
                <ITEXT CH=". Note that we pick a meaning for dē that makes sense in the context: they are loading up the boat."/>
                <trail PARENT="footnotes"/>
            </StoryText>
        </PAGEOBJECT>
