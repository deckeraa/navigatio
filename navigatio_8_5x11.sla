            <StoryText>
                <DefaultStyle/>
                <para PARENT="Latin"/>
                <ITEXT CH="[8]     Erat autem super illum fontem arbor mīrae lātitūdinis"/>
                <ITEXT FEATURES="inherit superscript" CH="1"/>
                <ITEXT CH=" in gīrum, nōn nimis altitūdinis, cooperta"/>
                <ITEXT FEATURES="inherit superscript" CH="2"/>
                <ITEXT CH=" avibus candidissimīs. In tantum"/>
                <ITEXT FEATURES="inherit superscript" CH="3"/>
                <ITEXT CH=" cooperuērunt illam ut folia et rāmī eius minimē vidērentur"/>
                <ITEXT FEATURES="inherit superscript" CH="4"/>
                <ITEXT CH=". [9] Cum haec vīdisset"/>
                <ITEXT FEATURES="inherit superscript" CH="5"/>
                <ITEXT CH=" vir Deī, coepit intrā sē cōgitāre et tractāre quidnam esset"/>
                <ITEXT FEATURES="inherit superscript" CH="6"/>
                <ITEXT CH=", aut quae causa fuisset"/>
                <ITEXT FEATURES="inherit superscript" CH="7"/>
                <ITEXT CH=", ut tanta multitūdō avium potuisset"/>
                <ITEXT FEATURES="inherit superscript" CH="8"/>
                <ITEXT CH=" esse in ūnā collēctiōne."/>
                <para PARENT="Latin"/>
                <ITEXT CH="[10]    In tantum"/>
                <ITEXT FEATURES="inherit superscript" CH="9"/>
                <ITEXT CH=" sibi erat taedium ut effūdisset"/>
                <ITEXT FEATURES="inherit superscript" CH="10"/>
                <ITEXT CH=" lacrimās, prōvolūtīs genibus"/>
                <ITEXT FEATURES="inherit superscript" CH="11"/>
                <ITEXT CH=", atque dēprecāsset Deum"/>
                <ITEXT FEATURES="inherit superscript" CH="12"/>
                <ITEXT CH=", dīcēns: [11] “Deus, cognitor incognitōrum et revēlātor absconditōrum omnium, tū scīs angustiam cordis meī"/>
                <ITEXT FEATURES="inherit superscript" CH="13"/>
                <ITEXT CH=": [12] dēprecor"/>
                <ITEXT FEATURES="inherit superscript" CH="14"/>
                <ITEXT CH=" tuam maiestātem, ut mihi peccātōrī dignēris per tuam magnam misericordiam revēlāre tuum sēcrētum, quod modo videō prae oculīs meīs. Nōn dignitātis aut meritī praesūmō, sed clēmentiae tuae.”"/>
                <trail PARENT="Latin"/>
            </StoryText>
        </PAGEOBJECT>
