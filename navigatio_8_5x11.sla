            </PageItemAttributes>
        </PAGEOBJECT>
        <PAGEOBJECT XPOS="118.0008" YPOS="35000.5882292994" OwnPage="83" ItemID="1700619054" PTYPE="4" WIDTH="540" HEIGHT="456.15923566879" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="0" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="1" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L540 0 L540 456.159 L0 456.159 L0 0 Z" copath="M0 0 L540 0 L540 456.159 L0 456.159 L0 0 Z" gXpos="118.0008" gYpos="35000.5882292994" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="-1" BACKITEM="-1">
