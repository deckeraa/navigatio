                <ITEXT FEATURES="inherit superscript" CH="4"/>
                <ITEXT CH=". [9] Cum haec vīdisset"/>
                <ITEXT FEATURES="inherit superscript" CH="5"/>
                <ITEXT CH=" vir Deī, coepit intrā sē cōgitāre et tractāre quidnam esset"/>
