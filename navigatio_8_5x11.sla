        <PAGEOBJECT XPOS="118.0008" YPOS="35833.7188178344" OwnPage="85" ItemID="1706541534" PTYPE="4" WIDTH="540" HEIGHT="330.459526114668" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="0" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="1" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L540 0 L540 330.46 L0 330.46 L0 0 Z" copath="M0 0 L540 0 L540 330.46 L0 330.46 L0 0 Z" gXpos="118.0008" gYpos="35833.7188178344" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="-1" BACKITEM="-1">
            <StoryText>
                <DefaultStyle/>
                <para/>
                <ITEXT CH="[6]       Tunc Sānctus Barrindus, explētīs sermōnibus"/>
                <ITEXT FEATURES="inherit superscript" CH="1"/>
                <ITEXT CH=" sānctī Brendānī, coepit nārrāre dē quādam īnsulā, dīcēns: [7] “Fīliolus meus Mernoc atque prōcūrātor"/>
                <ITEXT FEATURES="inherit superscript" CH="2"/>
                <ITEXT CH=" pauperum Chrīstī cōnfugit ā faciē meā et voluit esse sōlitārius.  Invenitque īnsulam iuxtā Montem Lapidis, nōmine Dēliciōsa. [8] Post multum vērō tempus nūntiātum"/>
                <ITEXT FEATURES="inherit superscript" CH="3"/>
                <ITEXT CH=" est mihi quod plūrēs monachōs sēcum habuisset"/>
                <ITEXT FEATURES="inherit superscript" CH="4"/>
                <ITEXT CH=", et Deus multa mīrābilia per illum ostendit.  Itaque perrēxī illūc, ut vīsitāssem"/>
                <ITEXT FEATURES="inherit superscript" CH="5"/>
                <ITEXT CH=" fīliolum meum."/>
                <para PARENT="Latin"/>
                <ITEXT CH="[9]      “Cumque appropinquāssem"/>
                <ITEXT FEATURES="inherit superscript" CH="6"/>
                <ITEXT CH=" iter trium diērum, in occursum mihi festīnāvit cum frātribus suīs: revēlāvit enim Dominus sibi adventum meum. [10] Nāvigantibus nōbīs in praedictam īnsulam occurrērunt obviam, sīcut exāmen apum, ex dīversīs cellulīs"/>
                <ITEXT FEATURES="inherit superscript" CH="7"/>
                <ITEXT CH=", frātrēs."/>
                <para PARENT="Latin"/>
            </StoryText>
        </PAGEOBJECT>
