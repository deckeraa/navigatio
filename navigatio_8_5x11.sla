        <PAGEOBJECT XPOS="118.0008" YPOS="39160.152" OwnPage="93" ItemID="1726913790" PTYPE="4" WIDTH="540" HEIGHT="324" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="0" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="1" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L540 0 L540 324 L0 324 L0 0 Z" copath="M0 0 L540 0 L540 324 L0 324 L0 0 Z" gXpos="118.0008" gYpos="39160.152" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="-1" BACKITEM="-1">
            <StoryText>
                <DefaultStyle/>
                <para/>
                <ITEXT CH="[30]     “Cum haec audīssem"/>
                <ITEXT FEATURES="inherit superscript" CH="1"/>
                <ITEXT CH=", coepī illōs cōnfortāre, dīcēns eīs: ‘Nōlīte"/>
                <ITEXT FEATURES="inherit superscript" CH="2"/>
                <ITEXT CH=", frātrēs, putāre aliquid nisi bonum. Vestra conversātiō procul dubiō est ante portam"/>
                <ITEXT FEATURES="inherit superscript" CH="3"/>
                <ITEXT CH=" paradīsī. [31] Hīc prope est īnsula quae vocātur Terra Reprōmissiōnis Sānctōrum, ubi nec nox imminet nec diēs fīnītur. [32] Illūc frequentātur"/>
                <ITEXT FEATURES="inherit superscript" CH="4"/>
                <ITEXT CH=" abbās Mernoc; angelī enim Dominī cūstōdiunt illam. Nōnne cognōscitis in odōre vestīmentōrum nostrōrum quod in paradīsō Deī fuimus?’"/>
                <para PARENT="Latin"/>
                <ITEXT CH="[33]     “Tunc frātrēs respondērunt, dīcentēs: ‘Abbā, nōvimus quia fuistī in paradīsō Deī; spatium maris, ubi est ille paradīsus, ignōrāmus. [34] Nam saepe fragrānciam vestīmentōrum abbātis nostrī probāvimus: paene usque ad quadrāgintā diēs tenēbantur ab odōre.’"/>
                <para PARENT="Latin"/>
            </StoryText>
        </PAGEOBJECT>
        <PAGEOBJECT XPOS="118.0008" YPOS="39974.1552" OwnPage="95" ItemID="1730051934" PTYPE="4" WIDTH="540" HEIGHT="18" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="0" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="1" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L540 0 L540 18 L0 18 L0 0 Z" copath="M0 0 L540 0 L540 18 L0 18 L0 0 Z" gXpos="118.0008" gYpos="38310.1552" gWidth="0" gHeight="0" LAYER="0" NEXTITEM="-1" BACKITEM="-1">
